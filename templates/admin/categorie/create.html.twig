{% extends 'layout.html.twig' %}

{% block title %}Créer une Catégorie{% endblock %}

{% block admin %}
<div class="row">
    <div class="col-lg-12">
        <div id="content-header" class="clearfix">
            <div class="float-left">
                <ol class="breadcrumb">
                    <li><a href="#">Home</a></li>
                    <li class="active"><span>User</span></li>
                    <li class="active"><span>Create</span></li>
                </ol>
            </div>
        </div>
    </div>
</div>
<div class="row">
    <div class="col-lg-6">
        <div class="main-box">
            <header class="main-box-header clearfix">
                <h2>Add Categorie</h2>
            </header>
            <div class="main-box-body clearfix">
                {{form_start(form)}}
                {{form_row(form.name)}}
                {{form_row(form.submit)}}
                {{form_end(form)}}
            </div>
        </div>
    </div>
</div>
{% endblock %}
