                                                    <i class="fas fa-video"></i>
                                                    <span>Videos</span>
                                                </a>
                                                <ul class="submenu">
                                                    <li>
                                                        <a href="{{path('app_video_index')}}">
                                                            Video Lists
                                                            <span class="badge badge-success float-right">Lists</span>
                                                        </a>
                                                    </li>
                                                </ul>
                                            </li>
                                        </ul>
                                    </div>
                                </div>
                            </div>
                        </section>
                        <div id="nav-col-submenu"></div>
