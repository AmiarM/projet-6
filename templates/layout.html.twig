<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0" />
        <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
        <title>
            {% block title %}
            Tableau de Bord | SnowTricks
            {% endblock %}
        </title>
        {% block stylesheet %}
        {% endblock %}
        {% block javascript %} {% endblock %}
        <link rel="stylesheet" type="text/css" href="{{ asset('css/bootstrap.min.css') }}" />

   
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.1.1/css/all.min.css" integrity="sha512-KfkfwYDsLkIlwQp6LFnl8zNdLGxu9YAA1QvwINks4PhcElQSvqcyVLLD9aMhXd13uQjoXtEKNosOWaZqXgel0g==" crossorigin="anonymous" referrerpolicy="no-referrer" />

        <link rel="stylesheet" type="text/css" href="{{ asset('css/nanoscroller.css') }}" />

        <link rel="stylesheet" type="text/css" href="{{ asset('css/theme_styles.css') }}" />

        <link rel="stylesheet" type="text/css" href="{{ asset('css/jquery-jvectormap-2.0.3.css') }}" />
        <link rel="stylesheet" type="text/css" href="{{ asset('css/weather-icons.min.css') }}" />

        

        <link href='//fonts.googleapis.com/css?family=Open+Sans:400,600,700,300' rel='stylesheet' type='text/css'>
    </head>
    <body>
            {% if app.user  %}
            <div id="theme-wrapper">
                <header class="navbar navbar-expand-lg navbar-light container d-block d-lg-flex" id="header-navbar">
                    <a class="navbar-brand float-left float-lg-none" href="index.html" id="logo">
                        <img alt="" class="normal-logo logo-white" src="{{ asset('images/logo.png') }}" />
                        <img alt="" class="normal-logo logo-black" src="{{ asset('images/logo-black.png') }}" />
                    </a>
                    <button aria-controls="navbar-ex1-collapse" aria-expanded="false" aria-label="Toggle navigation" class="navbar-toggler float-right float-lg-none" data-target=".navbar-ex1-collapse" data-toggle="collapse" type="button">
                        <span class="fa fa-bars"></span>
                    </button>
                    <ul class="nav navbar-nav mr-auto d-none d-lg-block mrg-l-none">
                        <li class="dropdown d-none d-md-block">
                            <a class="btn dropdown-toggle" data-toggle="dropdown">
                                Administration du site
                            </a>
                            <ul class="dropdown-menu">
                                <li class="item">
                                    <a href="{{path('app_categorie_add')}}">
                                        <i class="fa fa-sitemap">
                                        </i>
                                        Ajouter Categorie
                                    </a>
                                </li>
                                <li class="item">
                                    <a href="{{path('app_trick_new')}}">
                                        <i class="fa fa-snowboarding">
                                        </i>
                                        Ajouter Trick
                                    </a>
                                </li>
                                <li class="item">
                                    <a href="{{path('app_comment_new')}}">
                                        <i class="fa fa-comments">
                                        </i>
                                        Ajouter un Commentaire
                                    </a>
                                </li>
                            </ul>
                        </li>
        
                    </ul>
                    <ul class="nav navbar-nav ml-auto float-right float-lg-none" id="header-nav">
                        <li class="mobile-search">
                            <a class="btn">
                                <i class="fa fa-search">
                                </i>
                            </a>
                            <div class="drowdown-search">
                                <form role="search">
                                    <div class="form-group">
                                        <input class="form-control" placeholder="Search..." type="text">
                                        <i class="fa fa-search nav-search-icon">
                                        </i>
                                        </input>
                                    </div>
                                </form>
                            </div>
                        </li>
                        <li class="dropdown profile-dropdown">
                            <a class="dropdown-toggle" data-toggle="dropdown" href="#">
                                <img alt="" src="{{ app.user.avatar}}" />
                                <span class="d-none d-md-block">
                                    {{app.user.lastname|upper}} {{app.user.firstname|slice(0,1)|upper}}
                                </span>
                                <b class="caret">
                                </b>
                            </a>
                            <ul class="dropdown-menu dropdown-menu-right">
                                <li>
                                    <a href="{{path('app_user_account')}}">
                                        <i class="fa fa-user">
                                        </i>
                                        Profile
                                    </a>
                                </li>
                                <li>
                                    <a href="{{path('app_user_logout')}}">
                                        <i class="fa fa-power-off">
                                        </i>
                                        Déconnexion
                                    </a>
                                </li>
                            </ul>
                        </li>
                        <li class="d-none d-sm-block">
                            <a class="btn" href="{{path('app_user_logout')}}">
                                <i class="fa fa-power-off">
                                </i>
                            </a>
                        </li>
                    </ul>
                </header>
                <div id="page-wrapper" class="container">
                    <div class="row">
                        <div id="nav-col">
                            <section id="col-left" class="col-left-nano">
                                <div id="col-left-inner" class="col-left-nano-content">
                                    <div id="user-left-box" class="clearfix d-none d-lg-block profile2-dropdown">
                                        <img alt="" src="{{app.user.avatar}}"/>
                                        <div class="user-box">
                                            <span class="dropdown name">
                                                <a href="#" class="dropdown-nocaret dropdown-toggle" data-toggle="dropdown">
                                                    {{app.user.lastname|upper}} {{app.user.firstname|slice(0,1)|upper}}.
                                                    <i class="fa fa-angle-down"></i>
                                                </a>
                                                <ul class="dropdown-menu dropdown-menu-right">
                                                    <li class="item"><a href="{{path('app_user_account')}}"><i class="fa fa-user"></i>Profile</a></li>
                                                    <li class="item"><a href="{{path('app_user_logout')}}"><i class="fa fa-power-off"></i>Déconnexion</a></li>
                                                </ul>
                                            </span>
                                        </div>
                                    </div>
                                    <div class="navbar navbar-expand-lg navbar-light">
                                        <div class="collapse navbar-collapse navbar-ex1-collapse" id="sidebar-nav">
                                            <ul class="nav navbar-nav nav-pills nav-stacked">
                                                <li class="nav-header nav-header-first d-none d-lg-block">
                                                    Navigation
                                                </li>
                                                <li class="active">
                                                    <a href="{{path('app_admin')}}">
                                                        <i class="fa fa-dashboard"></i>
                                                        <span>Tableau de Bord</span>
                                                    </a>
                                                </li>
                                                <li>
                                                    <a href="#" class="dropdown-toggle dropdown-nocaret">
                                                        <i class="fa fa-snowboarding"></i>
                                                        <span>Tricks</span>
                                                        <i class="fa fa-angle-right drop-icon"></i>
                                                    </a>
                                                    <ul class="submenu">
                                                        <li>
                                                            <a href="{{path('app_trick_new')}}">
                                                               Ajouter
                                                                <span class="badge badge-success float-right">New</span>
                                                            </a>
                                                        </li>
                                                        {% for role in app.user.roles %}
                                                        {% endfor %}
                                                        <li>
                                                            <a href="{{path('app_tricks_lists')}}">
                                                                Mes Tricks 
                                                                <span class="badge badge-success float-right">Lists</span>
                                                            </a>
                                                        </li>
                                                    </ul>
                                                </li>   
                                                <li>
                                                    <a href="#" class="dropdown-toggle dropdown-nocaret">
                                                        <i class="fa fa-tag"></i>
                                                        <span>Categories</span>
                                                        <i class="fa fa-angle-right drop-icon"></i>
                                                    </a>
                                                    <ul class="submenu">
                                                        <li>
                                                            <a href="{{path('app_categorie_add')}}">
                                                                Ajouter
                                                                <span class="badge badge-success float-right">New</span>
                                                            </a>
                                                        </li>
                                                        <li>
                                                            <a href="{{path('app_categories')}}">
                                                                Liste
                                                                <span class="badge badge-success float-right">Lists</span>
                                                            </a>
                                                        </li>
                                                    </ul>
                                                </li>
                                                <li>
                                                    <a href="#" class="dropdown-toggle dropdown-nocaret">
                                                        <i class="fa fa-envelope"></i>
                                                        <span>Commentaires</span>
                                                        <i class="fa fa-angle-right drop-icon"></i>
                                                    </a>
                                                    <ul class="submenu">
                                                        <li>
                                                            <a href="{{path('app_comment_lists')}}">
                                                                Liste
                                                                <span class="badge badge-success float-right">Lists</span>
                                                            </a>
                                                        </li>
                                                    </ul>
                                                </li>  
                                            </ul>
                                        </div>
                                    </div>
                                </div>
                            </section>
                            <div id="nav-col-submenu"></div>
                        </div>
                        <div id="content-wrapper">
                            <div class="row">
                                <div class="col-lg-12">
                    {% block admin %} {% endblock %}
        
                                </div>
                            </div>
                        </div>
                    </div>
                    <div id="config-tool" class="closed">
                        <a id="config-tool-cog">
                            <i class="fa fa-cog"></i>
                        </a>
                        <div id="config-tool-options">
                            <h4>Layout Options</h4>
                            <ul>
                                <li>
                                    <div class="checkbox-nice">
                                        <input type="checkbox" id="config-fixed-header" />
                                        <label for="config-fixed-header">
                                            Fixed Header
                                        </label>
                                    </div>
                                </li>
                                <li>
                                    <div class="checkbox-nice">
                                        <input type="checkbox" id="config-fixed-sidebar" />
                                        <label for="config-fixed-sidebar">
                                            Fixed Left Menu
                                        </label>
                                    </div>
                                </li>
                                <li>
                                    <div class="checkbox-nice">
                                        <input type="checkbox" id="config-fixed-footer" />
                                        <label for="config-fixed-footer">
                                            Fixed Footer
                                        </label>
                                    </div>
                                </li>
                                <li>
                                    <div class="checkbox-nice">
                                        <input type="checkbox" id="config-boxed-layout" />
                                        <label for="config-boxed-layout">
                                            Boxed Layout
                                        </label>
                                    </div>
                                </li>
                                <li>
                                    <div class="checkbox-nice">
                                        <input type="checkbox" id="config-rtl-layout" />
                                        <label for="config-rtl-layout">
                                            Right-to-Left
                                        </label>
                                    </div>
                                </li>
                            </ul>
                            <br />
                            <h4>Skin Color</h4>
                            <ul id="skin-colors" class="clearfix">
                                <li>
                                    <a class="skin-changer" data-skin="" data-toggle="tooltip" title="Default" style="background-color: #34495e;">
                                    </a>
                                </li>
                                <li>
                                    <a class="skin-changer" data-skin="theme-white" data-toggle="tooltip" title="White/Green" style="background-color: #2ecc71;">
                                    </a>
                                </li>
                                <li>
                                    <a class="skin-changer blue-gradient" data-skin="theme-blue-gradient" data-toggle="tooltip" title="Gradient">
                                    </a>
                                </li>
                                <li>
                                    <a class="skin-changer" data-skin="theme-turquoise" data-toggle="tooltip" title="Green Sea" style="background-color: #1abc9c;">
                                    </a>
                                </li>
                                <li>
                                    <a class="skin-changer" data-skin="theme-amethyst" data-toggle="tooltip" title="Amethyst" style="background-color: #9b59b6;">
                                    </a>
                                </li>
                                <li>
                                    <a class="skin-changer" data-skin="theme-blue" data-toggle="tooltip" title="Blue" style="background-color: #2980b9;">
                                    </a>
                                </li>
                                <li>
                                    <a class="skin-changer" data-skin="theme-red" data-toggle="tooltip" title="Red" style="background-color: #e74c3c;">
                                    </a>
                                </li>
                                <li>
                                    <a class="skin-changer" data-skin="theme-whbl" data-toggle="tooltip" title="White/Blue" style="background-color: #3498db;">
                                    </a>
                                </li>
                            </ul>
                        </div>
                    </div>
            {% endif %}
           
            <script src=''{{ asset('js/demo-skin-changer.js') }}></script> 
            <script src="{{ asset('js/jquery.min.js') }}"></script>
            <script src="{{ asset('js/popper.min.js') }}"></script>
            <script src="{{ asset('js/bootstrap.js') }}"></script>
            <script src="{{ asset('js/jquery.nanoscroller.min.js') }}"></script>
            <script src="{{ asset('js/moment.min.js') }}"></script>
            <script src="{{ asset('js/jquery-jvectormap-2.0.3.min.js') }}"></script>
            <script src="{{ asset('js/jquery-jvectormap-world-merc.js') }}"></script>
            <script src="{{ asset('js/gdp-data.js') }}"></script>
            <!--[if lte IE 8]><script language="javascript" type="text/javascript" src="components/flot/excanvas.min.js"></script><![endif]-->
            <script src="{{ asset('js/jquery.flot.js') }}"></script>
            <script src="{{ asset('js/jquery.flot.resize.js') }}"></script>
            <script src="{{ asset('js/jquery.flot.time.js') }}"></script>
            <script src="{{ asset('js/jquery.flot.threshold.js') }}"></script>
            <script src="{{ asset('js/jquery.flot.axislabels.js') }}"></script>
            <script src="{{ asset('js/jquery.sparkline.min.js') }}"></script>
            <script src="{{ asset('js/skycons.js') }}"></script>

            <script src="{{ asset('js/scripts.js') }}"></script>
            <script src="{{ asset('js/pace.min.js') }}"></script>

            <script src="{{ asset('js/script.js') }}"></script>
            <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
        </div>
    </body>
</html>