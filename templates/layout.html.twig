                    <div id="content-wrapper">
                        <div class="row">
                            <div class="col-lg-12">
                {% block admin %} {% endblock %}
    
                                <footer id="footer-bar" class="row">
                                    <div class="container">
                                        <p id="footer-copyright" class="col-12">
                                            Powered by Cube Theme.
                                        </p>
